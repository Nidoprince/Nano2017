   <text:p text:style-name="P3"><text:span text:style-name="T8"/></text:p>
   <text:p text:style-name="P3"><text:span text:style-name="T8"/></text:p>
   <text:p text:style-name="P3"><text:span text:style-name="T8"/></text:p>
   <text:p text:style-name="P3"><text:span text:style-name="T8"/></text:p>
